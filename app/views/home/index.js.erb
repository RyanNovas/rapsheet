$("#map-container").empty();

var minimums =  <%= @minimums %>;
var maximums =  <%= @maximums %>;
var states = <%= @states %>;

dataManipulators(minimums, maximums, states);


function playMap(){
  var year = $("#form_year");

  while (year <= 2012) {
    window.setInterval({year += 1;}, 1000)
    $("#form_year").val(year);
    $("#crime_form").submit();
    dataManipulators(minimums, maximums, states)
  }
};
